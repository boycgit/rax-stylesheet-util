{
  "name": "rax-stylesheet-util",
  "version": "0.0.3",
  "description": "rax stylesheet utils",
  "main": "dist/index.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "browser": {
    "./index": "index.browser.js"
  },
  "size-limit": [
    {
      "path": "build/index.js"
    }
  ],
  "scripts": {
    "watch": "webpack --watch --config webpack.dev.js",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "size": "size-limit",
    "test": "jest && eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boycgit/rax-stylesheet-util.git"
  },
  "keywords": [
    "rax",
    "stylesheet",
    "util"
  ],
  "author": "boycgit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boycgit/rax-stylesheet-util/issues"
  },
  "homepage": "https://github.com/boycgit/rax-stylesheet-util#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "jest": "^22.4.3",
    "size-limit": "^0.17.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "camelcase": "^5.0.0",
    "chalk": "^2.3.2",
    "css": "^2.2.1",
    "debug": "^3.1.0"
  }
}
